{
  "name": "example-axum-app",
  "version": "1.0.0",
  "description": "An example Axum app using the axum-napi-bridge.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "napi build --release",
    "test": "ava",
    "test:e2e": "playwright test",
    "bench": "node --loader ts-node/esm --import @oxc-node/core/register benchmark/bridge_bench.ts",
    "bench:concurrent": "node benchmark/concurrent_bench.mjs"
  },
  "dependencies": {
    "@napi-rs/cli": "^3.1.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "node-fetch": "^3.3.2",
    "ava": "^5.3.1",
    "ts-node": "^10.9.1",
    "tinybench": "^2.4.0",
    "typescript": "^5.2.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "tests/bridge.spec.ts"
    ]
  }
}
