{
  "name": "example-axum-app",
  "version": "1.0.0",
  "description": "An example Axum app using the axum-napi-bridge.",
  "main": "index.js",
  "scripts": {
    "build": "napi build --release",
    "test": "ava",
    "test:e2e": "playwright test",
    "bench": "npx tsc benchmark/bridge_bench.ts --outDir benchmark --target es2022 --module es2022 --esModuleInterop --skipLibCheck --moduleResolution node && node benchmark/bridge_bench.js",
    "bench:concurrent": "node benchmark/concurrent_bench.mjs"
  },
  "dependencies": {
    "@napi-rs/cli": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/koa": "^3.0.0",
    "express": "^5.1.0",
    "koa": "^3.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "ava": "^5.3.1",
    "node-fetch": "^3.3.2",
    "tinybench": "^2.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "tests/bridge.spec.ts"
    ]
  }
}
